{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "./codestyle.json",
    "./overrides.json"
  ],
  "parser": "eslint-plugin-typescript/parser",
  "plugins": ["typescript", "import"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  }
}
